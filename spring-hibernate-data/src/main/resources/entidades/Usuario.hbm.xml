<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" package="com.origen.hibernate.model">
  <class name="Usuario" table="INTUSUARIO_SEG">
    <id column="USER_ID" name="id">
      <generator class="native"/>
    </id>
    <property column="USER_NOMBR" name="name"/>
    <property column="USER_APELL" name="country"/>
    <property column="USER_EMAIL" name="userEmail"/>
  </class>
</hibernate-mapping>
